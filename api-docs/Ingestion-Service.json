              "type": "string",
              "paramType": "path",
              "allowMultiple": false
            },
            {
              "name": "articleId",
              "description": "articleId",
              "required": true,
              "type": "PathSegment",
              "paramType": "path",
              "allowMultiple": false
            },
            {
              "name": "renditionName",
              "description": "renditionName",
              "required": true,
              "type": "string",
              "paramType": "path",
              "allowMultiple": false
            }
