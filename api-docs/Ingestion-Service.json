  "resourcePath": "/Article-Ingestion",
  "apis": [
    {
      "path": "/publication{publicationId}/article/{articleId}/contents/{renditionName}",
      "operations": [
        {
          "method": "PUT",
          "summary": "This method will upload the contents of a DPS 2015 .article file into the Adobe Digital Publishing Solution environment. This method requires the entityType to be 'article',content-type to be 'application/zip' or 'application/vnd.adobe.article+zip' and requires valid values for the headers.It also requires a non-null and non-empty article file. This method operates asynchronously, meaning a '202 ACCEPTED'response will be returned as soon as the article contents are uploaded. The bits of the article zip file are processed and ingested asynchronously by means of a workflow process. The status updates of this asynchronous process are posted to the PECS Status API by the workflow. Any updates can by obtained by querying the Status API.",
          "notes": "",
          "type": "void",
          "nickname": "uploadFullArticle",
          "produces": [
            "application/json"
          ],
          "consumes": [
            "application/vnd.adobe.article+zip,application/zip"
          ],
          "parameters": [
            {
              "name": "x-dps-client-session-id",
              "required": false,
              "type": "string",
              "paramType": "header",
              "allowMultiple": false
            },
            {
